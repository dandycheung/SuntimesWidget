<?xml version="1.0" encoding="UTF-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:color="@color/alarm_enabled_dark"
    tools:targetApi="lollipop">
    <item>
        <shape>
            <solid android:color="@color/card_bg_dark" />
            <corners android:radius="@dimen/alarmcard_corners_pressed" />
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
            <corners android:radius="@dimen/alarmcard_corners_pressed" />
        </shape>
    </item>
</ripple>